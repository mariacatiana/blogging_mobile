{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-native", 
    "esModuleInterop": true, 
    "allowSyntheticDefaultImports": true, 
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "skipLibCheck": true 
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}

